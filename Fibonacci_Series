class FibonacciSeries {
    public static void main(String[] args) {
        int a = 0;
        int b=1;
        int num = 10;
        System.out.println("Fibonacci series :");
        for(int i=1; i<num; i++) {
            System.out.print(a + " ");
        int sum = a+b;
        a= b;
        b= sum;
        }
    }
}
