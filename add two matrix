// add two matrix



public class Arrays {
   public static void main(String args[]) {
   int [][] a1 = {{1,2,3},{4,5,6}};
   int [][] a2 = {{7,8,9},{10,11,12}};
   int [][] a3 = new int[2][3];
   for(int i=0;i<2;i++) {
      for(int j=0;j<3;j++) {
    a3[i][j] = a1[i][j] + a2[i][j];
    System.out.print(a3[i][j] + " ");

   }
   System.out.println();

  }
}
}

    
