// check armstrong number
import java.util.*;
class external {
public static void main(String[] args) {
Scanner sc = new Scanner(System.in);
int arm =0;
int temp;
System.out.println("Enter a number");
int num = sc.nextInt();

while(num>0) {
    int rem = num%10;
    arm = (rem * rem * rem) + arm;
    num = num/10;

}
if(num==arm) {
    System.out.println("It is a armstrong");
} else {
    System.out.println("It is not a armstrong");
}
} 
}
